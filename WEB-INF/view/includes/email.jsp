<%@ page contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" trimDirectiveWhitespaces="true" %>

<div id="email" class="d-none">

	<div class="form-group">
		<label for="emailSetup">Have you setup your email?</label>
		<div class="form-check form-check-inline">
			<input name="emailSetup" class="form-check-input" type="radio" value="Yes">
			<label class="form-check-label mr-1" for="emailSetup">
				Yes
			</label>
			<input name="emailSetup"  class="form-check-input" type="radio" value="No">
			<label class="form-check-label" for="emailSetup">
				No
			</label>
		</div>
	</div>
	<div class="form-group">
		<label for="emailSignIn">Can you sign in?</label>
		<div class="form-check form-check-inline">
			<input name="emailSignIn" class="form-check-input" type="radio" value="Yes">
			<label class="form-check-label mr-1" for="emailSignIn">
				Yes
			</label>
			<input name="emailSignIn"  class="form-check-input" type="radio" value="No">
			<label class="form-check-label" for="emailSignIn">
				No
			</label>
		</div>
	</div>
	<div class="form-group">
		<label for="emailReset">Have you tried emailResetting your password?</label>
		<div class="form-check form-check-inline">
			<input name="emailReset" class="form-check-input" type="radio" value="Yes">
			<label class="form-check-label mr-1" for="emailReset">
				Yes
			</label>
			<input name="emailReset"  class="form-check-input" type="radio" value="No">
			<label class="form-check-label" for="emailReset">
				No
			</label>
		</div>
	</div>
	<div class="form-group">
		<label for="emailSendReceive">Can you send and receive emails?</label>
		<div class="form-check form-check-inline">
			<input name="emailSendReceive" class="form-check-input" type="radio" value="Yes">
			<label class="form-check-label mr-1" for="emailSendReceive">
				Yes
			</label>
			<input name="emailSendReceive"  class="form-check-input" type="radio" value="No">
			<label class="form-check-label" for="emailSendReceive">
				No
			</label>
		</div>
	</div>
	<div class="form-group">
		<label for="emailInternet">Have you confirmed your emailInternet connection?</label>
		<div class="form-check form-check-inline">
			<input name="emailInternet" class="form-check-input" type="radio" value="Yes">
			<label class="form-check-label mr-1" for="emailInternet">
				Yes
			</label>
			<input name="emailInternet"  class="form-check-input" type="radio" value="No">
			<label class="form-check-label" for="emailInternet">
				No
			</label>
		</div>
	</div>
</div>
